<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="16dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_lokasi_dnote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dummy Location"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_margin="4dp"/>
    <TextView
        android:id="@+id/tv_waktu_dnote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00:00 - 00:00"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
        app:layout_constraintTop_toBottomOf="@id/tv_lokasi_dnote"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_margin="4dp"/>
    <TextView
        android:id="@+id/tv_ket_dnote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dummy Keterangan"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
        app:layout_constraintTop_toBottomOf="@id/tv_waktu_dnote"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_margin="4dp"/>
    <TextView
        android:id="@+id/tv_alasan_dnote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dummy Alasan"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
        app:layout_constraintTop_toBottomOf="@id/tv_ket_dnote"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_margin="4dp"/>
    <TextView
        android:id="@+id/tv_status_dnote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dummy Status"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
        app:layout_constraintTop_toBottomOf="@id/tv_alasan_dnote"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_margin="4dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>